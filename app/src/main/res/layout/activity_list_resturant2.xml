<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D7FFAB40"
    tools:context=".List_resturant">

    <androidx.cardview.widget.CardView
        android:layout_width="352dp"
        android:layout_height="680dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="67dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="16dp"
        android:background="#BCFFD740"
        app:cardBackgroundColor="#BCEBD78E"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.227">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="231dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="70dp"
            android:layout_marginTop="40dp"
            android:layout_marginBottom="8dp"
            android:background="#BEFFD740"
            android:text="ResturantList"
            android:textSize="36sp" />

        <androidx.cardview.widget.CardView
            android:layout_width="152dp"
            android:layout_height="123dp"
            android:layout_marginStart="177dp"
            android:layout_marginTop="500dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="96dp">

            <TextView
                android:id="@+id/textView8"
                android:layout_width="110dp"
                android:layout_height="46dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="40dp"
                android:singleLine="false"
                android:text="BEVERAGE"
                android:textSize="22sp" />

        </androidx.cardview.widget.CardView>

        <SearchView
            android:id="@+id/searchView"
            android:layout_width="195dp"
            android:layout_height="32dp"
            android:layout_marginStart="90dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="120dp"
            android:layout_marginRight="169dp"
            android:layout_marginBottom="476dp"
            android:background="#FDFFFFFF"
            app:layout_constraintBottom_toTopOf="@+id/textView2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <com.google.android.material.textfield.TextInputEditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="hint" />
            </com.google.android.material.textfield.TextInputLayout>
        </SearchView>

        <androidx.cardview.widget.CardView
            android:layout_width="146dp"
            android:layout_height="119dp"
            android:layout_marginStart="182dp"
            android:layout_marginTop="190dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="384dp">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="110dp"
                android:layout_height="46dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="40dp"
                android:singleLine="false"
                android:text="JAPANESS"
                android:textSize="22sp" />
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="148dp"
            android:layout_height="118dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="190dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="384dp">

            <TextView
                android:id="@+id/fastfood"
                android:layout_width="128dp"
                android:layout_height="48dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="40dp"

                android:singleLine="false"
                android:text="FASTFOOD"
                android:textSize="22sp" />
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="147dp"
            android:layout_height="122dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="350dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="240dp">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="110dp"
                android:layout_height="46dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="40dp"
                android:singleLine="false"
                android:text="THAIFOOD"
                android:textSize="22sp" />
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="144dp"
            android:layout_height="115dp"
            android:layout_marginStart="177dp"
            android:layout_marginTop="350dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="240dp">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="110dp"
                android:layout_height="46dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="40dp"
                android:singleLine="false"
                android:text="CHAINESS"
                android:textSize="22sp" />
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="145dp"
            android:layout_height="129dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="500dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="96dp">

            <TextView
                android:id="@+id/textView7"
                android:layout_width="110dp"
                android:layout_height="46dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="40dp"
                android:singleLine="false"
                android:text="DESSERT"
                android:textSize="22sp" />
        </androidx.cardview.widget.CardView>

    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>